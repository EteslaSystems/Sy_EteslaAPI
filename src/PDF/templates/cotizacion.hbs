        /*----------------Coloreada de celdas------------*/
        function pintarRejasFinanciamiento(){
            var celdaAhorroMensual = document.getElementById('tdAhorroMensual');
            var ahorroMensual = celdaAhorroMensual.textContent;
            ahorroMensual = ahorroMensual.replace(/[$]/g,'');
            ahorroMensual = parseFloat(ahorroMensual);
            var montoDiezPorciento = (monto) => {
                var cantidadDiezPorc = Math.round(((monto * 10) / 100) * 100) / 100;
                cantidadDiezPorc = monto + cantidadDiezPorc;
                return cantidadDiezPorc;
            };
            montoDiezPorciento = montoDiezPorciento(ahorroMensual);

            var meses = ['doce','veinticuatro','treintaseis','cuarentaocho','sesenta','setentados','ochentacuatro'];        
            var numeroMeses = ['15','35','50'];

            for(var i=0; i<meses.length; i++)
            {
                for(var j=0; j<3; j++)
                {
                    idCelda = meses[i] +'_'+ numeroMeses[j];
                    idCelda = idCelda.toString();

                    var celdaFinanciamiento = document.getElementById(idCelda);
                    /*Formating*/
                    var montoFinanciamiento = celdaFinanciamiento.textContent;
                    montoFinanciamiento = montoFinanciamiento.replace(/[$]/g,'');
                    montoFinanciamiento = parseFloat(montoFinanciamiento);
                    
                    var hxColor = '';

                    //Validacion para colorear celda
                    if(montoFinanciamiento > ahorroMensual && montoFinanciamiento < montoDiezPorciento){ //Amarillo
                        hxColor = '#E0D30C';
                    }
                    else if(montoFinanciamiento <= ahorroMensual){
                        hxColor = '#44C331';
                    }
                    else{
                        hxColor = '#3A565E';
                    }

                    celdaFinanciamiento.style.backgroundColor = hxColor;
                }
            }
        }
    </script>
