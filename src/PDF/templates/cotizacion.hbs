                                            <td class="etqTexto" align="center">{{ objCombinaciones.combinacionMediana.0.paneles.noModulos }}</td>
                                            <td class="etqTexto">Kit de estructuras de aluminio especiales para modulos fotovoltaicos</td>
                                            <td id="tdPrecioEstructuras" class="etqTexto" align="center">{{ objCombinaciones.combinacionMediana.0.paneles.costoDeEstructuras }}$</td>
                                        </tr>
                                    {{/if}}
                                    {{#if combinacionOptima}}
                                        <tr>
                                            <td id="tdCantidadPaneles" class="etqTexto" align="center">{{ objCombinaciones.combinacionOptima.0.paneles.noModulos }}</td>
                                            <td class="etqTexto">{{ objCombinaciones.combinacionOptima.0.paneles.nombre }}</td>
                                            <td id="tdPrecioPaneles" class="etqTexto" align="center">{{ objCombinaciones.combinacionOptima.0.paneles.costoTotal }}$</td>
                                        </tr>
                                        <tr>
                                            {{#if objCombinaciones.combinacionOptima.0.inversores.combinacion}}
                                                <td id="tdCantidadInversores" class="etqTexto" align="center">{{ objCombinaciones.combinacionOptima.0.inversores.numeroDeInversores.cantidadTotalEquipos }}</td>
                                                <td class="etqTexto"><strong>QS1: </strong>{{ objCombinaciones.combinacionOptima.0.inversores.numeroDeInversores.invSoportMay }} <strong>YC600: </strong>{{ objCombinaciones.combinacionEconomica.0.inversores.numeroDeInversores.invSoportMen }}</td>
