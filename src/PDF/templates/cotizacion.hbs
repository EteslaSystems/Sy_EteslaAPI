        /*-------------G R A F I C O S-------------*/
        /* Scripts que pintan los graficos en las paginas*/
        function printGraphicsToPDF(){
            var graficoBenefEnergeticos = document.getElementById('grafBenefEnergeticos').getContext('2d');
            var graficoBenefFinancieros = document.getElementById('graficoBenefFinancieros').getContext('2d');
            
            var porcentajeEnergia = document.getElementById('tdPorcentajePotencia').textContent;
            porcentajeEnergia = porcentajeEnergia.replace(/[%]/g,'');
            porcentajeEnergia = parseFloat(porcentajeEnergia); //Formating
            sobrantePorcentaje = 100 - porcentajeEnergia;

            var configBenefEnerg = {
                type: 'pie',
                data: { 
                    datasets: [{
                        borderAlign: 'inner',
                        data: [porcentajeEnergia, sobrantePorcentaje],
                        backgroundColor: [ 'green', 'red' ]
                    }],
                    labels: [ 'Energia generada por el sistema (kwh)', 'Energia a pagar a CFE (kwh)' ]
                },
                options: {
                    animation: { animateRotate: false },
                    tooltips: { enabled: false },
                    responsive: true,
                    title:{
                        display: true,
                        position: 'left',
                        text: 'Ahorro energetico'
                    },
                    plugins: {
                        datalabels: {
                            backgroundColor: '#ccc',
                            borderRadius: 2,
                            formatter: (value) => {
                                return value + '%';
                            }
                        }
                    }
                }
            };
            
            /*--------------------------------*--------------------------------*/
            var gastoActualAnual = document.getElementById('tdGastoActualAnual').textContent;
            gastoActualAnual = gastoActualAnual.replace(/[$]/g,'');
            gastoActualAnual = parseFloat(gastoActualAnual);
            var proyeccionAnual = document.getElementById('tdProyeccionAnual').textContent;
            proyeccionAnual = proyeccionAnual.replace(/[$]/g,'');
            proyeccionAnual = parseFloat(proyeccionAnual);

            var configBenefFinan = {
                type: 'bar',
                data: { 
                    datasets: [{
                        label: 'Con paneles',
                        borderColor: 'grey',
                        borderWidth: 1,
                        data: [proyeccionAnual],
                        backgroundColor: [ 'green' ]
                    },
                    {
