                                            {{/if}}
                                            <td id="tdPrecioPrecioInversores" class="etqTexto" align="center">{{ objCombinaciones.combinacionEconomica.0.inversores.precioTotal }}$</td>
                                        </tr>
                                        <tr>
                                            <td class="etqTexto" align="center">{{ objCombinaciones.combinacionEconomica.0.paneles.noModulos }}</td>
                                            <td class="etqTexto">Kit de estructuras de aluminio especiales para modulos fotovoltaicos</td>
                                            <td id="tdPrecioEstructuras" class="etqTexto" align="center">{{ objCombinaciones.combinacionEconomica.0.paneles.costoDeEstructuras }}$</td>
                                        </tr>
                                    {{/if}}
                                    {{#if combinacionMediana}}
                                        <tr>
                                            <td id="tdCantidadPaneles" class="etqTexto" align="center">{{ objCombinaciones.combinacionMediana.0.paneles.noModulos }}</td>
                                            <td class="etqTexto">{{ objCombinaciones.combinacionMediana.0.paneles.nombre }}</td>
